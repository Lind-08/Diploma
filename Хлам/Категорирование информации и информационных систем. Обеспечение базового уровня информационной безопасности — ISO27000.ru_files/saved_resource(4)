			<script type="text/javascript">
				function loadRecommend(){
					var pathname = window.location.pathname;
					var pattern = /\/[a-z]{2}\//;
					var m = pathname.match(pattern);
					var param = '&lang=en-GB';
					if(m){
						if(m[0] == '/ru/'){
							param = '&lang=ru-RU';
						}else if(m[0] == '/es/'){
							param = '&lang=es-ES';
						}else if(m[0] == '/fr/'){
							param = '&lang=fr-FR';
						}else if(m[0] == '/it/'){
							param = '&lang=it-IT';
						}else if(m[0] == '/pt/'){
							param = '&lang=pt-BR';
						}else if(m[0] == '/de/'){
							param = '&lang=de-DE';
						}
					}
				
					$.ajax({
						url:homeUrl+'index.php',
						type : 'get',
						data :'com=index&t=loadRecommend'+param,
						dataType:'html',
						success:function(html){
							$('#recommendations .index_product_list').replaceWith(html);
						}
					});	
				}
				loadRecommend();
			</script>
			
			
