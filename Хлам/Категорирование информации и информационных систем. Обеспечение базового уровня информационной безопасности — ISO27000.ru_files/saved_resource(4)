	    	<script type="text/javascript" src="//static.criteo.net/js/ld/ld.js" async="true"></script>
	<script type="text/javascript">
$(function(){
	var account_id = 20046;
	var currency = 'USD';
	var customers_id = 0;
	var email = '';
	$.ajax({
		type: "get",
		dataType: "json",
		url: "index.php?com=ajax&t=getCurrentCountry",
		async: false,
		success: function(res){
			account_id = res.account_id;
			currency = res.currency;
			customers_id = res.customers_id;
    		email = res.email;
		}
    });
	window.criteo_q = window.criteo_q || [];
	window.criteo_q.push(
	        { event: "setAccount", account: account_id },
	        { event: "setCurrency", currency: currency},
	        { event: "setCustomerId", id: customers_id },
    		{ event: "setHashedEmail", email: email},
	        { event: "setSiteType", type: "d" },
	        { event: "viewHome" }
	);
});
	</script>
