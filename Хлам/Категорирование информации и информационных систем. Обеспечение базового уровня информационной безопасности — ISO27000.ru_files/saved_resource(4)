		$(".header").parent().before(topbanner);
		$(".topbanner i").click(function(){
			$(this).parents(".wrap").remove();
			$.cookie("topbanner_1", "close", {expires:1});
		});
		if(!($.cookie("topbanner_1") ===  "close"))
		{
			setTimeout('$(".topbanner").slideDown(1000)',2000); 
		} 
		
