<script>

            EBTD.Calculator.Main('#main-page-calc', false, true);
        
            $(function() {
                try {
                    EBTD.Utils.collapser({
                        container: '.page-description-wrapper',
                        target: '.block-page-description',
                        trigger: '.description-button',
                        limit: 0,
                        visible: 'auto',
                        labels: ['Читать далее', 'Свернуть текст'],
                        scroll: true
                    });
                } catch (e) { }
            });
        
        window.APRT_DATA = {
            pageType: 1
        };

        window._retag = window._retag || [];
        
        window._retag.push({code: "9ce8886180", level: 0});
        (function () {
            var id = "admitad-retag";
            if (document.getElementById(id)) {return;}
                var s = document.createElement("script");
            s.async = true; s.id = id;
            var r = (new Date).getTime();
            s.src = (document.location.protocol == "https:" ? "https:" : "http:") + "//cdn.admitad.com/static/js/retag.min.js?r="+r;
            var a = document.getElementsByTagName("script")[0];
            a.parentNode.insertBefore(s, a);
        })();
        
    
    $(function() {
        var lazyBlocks =  $('.lazy');
        var offset = 200;

        var loadNextBlock = function() {
            if(!lazyBlocks.length) {
                return;
            }
            var container = lazyBlocks.first();
            if(container.data('status') == 'loading') {
                return;
            }
            var objectBottomPosition = container.position().top + container.outerHeight();
            var windowBottomPosition = $(window).scrollTop() + $(window).height();
            if(windowBottomPosition + offset > objectBottomPosition) {
                container.data('status', 'loading');
                $.ajax({
                    url: container.data('url'),
                    dataType: 'html',
                    success : function(response, status) {
                        lazyBlocks = lazyBlocks.slice(1);
                        container.html(response);
                        loadNextBlock();
                    }
                });
            }
        };

        $(window).scroll(function() {
            loadNextBlock();
        });
        loadNextBlock();
    });

</script>

