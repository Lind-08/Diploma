<script>
    var SiteEvent = {
        track : function(category, action, label) {
            if (typeof ga == 'function') {

                var allTrackers = ga.getAll();
                var trackerName = allTrackers[0].get('name');

                if (typeof(label) == 'undefined') {
                    label = '';
                }
                ga(trackerName + '.send', {
                    hitType: 'event',
                    eventCategory: category,
                    eventAction: action,
                    eventLabel: label
                });

            } else {
                console.log('ga not loaded: ' + category + ' - ' + action);
            }
        }
    };
</script>
        
    
    

