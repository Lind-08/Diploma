<div id="searchLine">
	<form action="/internet-magazin/search/" method="GET" id="searchProductForm">
			<input type="hidden" name="is_submit" value="Y" />
	<div class="fl">
			<input type="text" autocomplete="off" name="product_title" id="searchStr" class="searchWord" def="Поиск среди 10 000 товаров" value='' />
			<ul class="dropdown"></ul>
	</div>
	<div class="fr">
			<input type="button" value="Найти" id="searchFormBut" class="submit" style="cursor: pointer;" />
	</div>
	</form>
	<script type="text/javascript">
	$(document).ready(function(){
		$('#searchFormBut').click(function(){
			if ( $('#searchStr').val() != $('#searchStr').attr('def') && $('#searchStr').val() != '' )
				$('#searchProductForm').submit();
		});

		$('#searchProductForm input[name="product_title"], #searchProductForm input[name="product_price_from"], #searchProductForm input[name="product_price_to"]').keydown(
		function(event){
			if(event.keyCode == 38 || event.keyCode == 40)
        		 return false;
			if(event.keyCode == 13 && $('#searchLine ul.dropdown li.hover').length == 0){
				if ( $('#searchStr').val() == $('#searchStr').attr('def') )
					$('#searchStr').val('');
				$('#searchProductForm').submit();
			}
		});

		$('#searchStr').keyup(function(event){
			var dropdown = $(this).next('ul.dropdown');
			var hover = dropdown.children('li.hover');
			if(event.which==38){
				if(!hover.length)
					dropdown.children('li:last').addClass('hover');
				else if(!hover.prev('li').length){
					hover.removeClass('hover');
					dropdown.children('li:last').addClass('hover');
				}
				else {
					hover.prev('li').addClass('hover');
					hover.removeClass('hover');
				}
				return false;
			}
			else if(event.which==40){
				if(!hover.length)
					dropdown.children('li:first').addClass('hover');
				else if(!hover.next('li').length){
					hover.removeClass('hover');
					dropdown.children('li:first').addClass('hover');
				}
				else {
					hover.next('li').addClass('hover');
					hover.removeClass('hover');
				}
				return false;
			}
			else if(event.which==13){
				if(dropdown.css('display') == 'block')
					dropdown.children('li.hover').click();
			}
			else if($(this).attr('value').length > 1){
				dropdown.load('/ajax/search/autocomplete.php?text='+encodeURIComponent($(this).attr('value')), function(){
						if(dropdown.html().length > 0)
							dropdown.show();
						else
							dropdown.hide();
					});

			}
			else{
				dropdown.hide();
			}
		});

        $('#searchLine ul.dropdown li').live('click', function(){
        	$('#searchStr').val($(this).text());
        	$('#searchLine ul.dropdown li.hover').removeClass('hover');
        	$('#searchLine ul.dropdown').hide();
        	return false;
        });

        $('#searchLine ul.dropdown li').live('hover', function(){
        	if($('#searchLine ul.dropdown').css('display') == 'block'){
        		$(this).parent().children('li').removeClass('hover');
        		$(this).addClass('hover');
        	}
        	return false;
        });
	});
	</script>
</div>
