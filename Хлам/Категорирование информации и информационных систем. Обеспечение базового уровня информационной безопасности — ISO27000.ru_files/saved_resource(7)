   <script>
       $(document).ready(function(){
           var availableTags = [
                              {label:"Москва и область",value:"1"},
                              {label:"Иваново",value:"344615"},
                              {label:"Нижний Новгород",value:"9"},
                              {label:"Саранск",value:"2893701"},
                              {label:"Арзамас",value:"849863"},
                              {label:"Йошкар-Ола",value:"1498908"},
                              {label:"Нижний Тагил",value:"22"},
                              {label:"Смоленск",value:"259018"},
                              {label:"Волгоград",value:"11"},
                              {label:"Казань",value:"5"},
                              {label:"Новосибирск",value:"7"},
                              {label:"Тула",value:"64465"},
                              {label:"Волжский",value:"1000768"},
                              {label:"Краснодар",value:"18"},
                              {label:"Оренбург",value:"19"},
                              {label:"Тюмень",value:"17"},
                              {label:"Воронеж",value:"10"},
                              {label:"Красноярск",value:"6"},
                              {label:"Ульяновск",value:"4876806"},
                              {label:"Городец",value:"5052224"},
                              {label:"Кстово",value:"5052278"},
                              {label:"Рязань",value:"13"},
                              {label:"Екатеринбург",value:"3"},
                              {label:"Курск",value:"640738"},
                              {label:"Самара",value:"4"},
                              {label:"Челябинск",value:"15"},
                              {label:"Заволжье",value:"5069514"},
                              {label:"Санкт-Петербург",value:"2"},
                              {label:"Ярославль",value:"16"},
                              {label:"",value:""}
           ];


           $("#other_city").autocomplete({
               //source: availableTags
               source: function( request, response ) {
                   var matcher = new RegExp( "^" + $.ui.autocomplete.escapeRegex( request.term ), "i" );
                   response( $.grep(availableTags, function( item ){
                       return matcher.test( item.label );
                   }) );
               },
               focus: function(event, ui) {
                   event.preventDefault();
                   $("#other_city").val(ui.item.label);
               },
               select: function( event, ui ) {
                   if(ui.item.value != $("#region_form_input").val())
                   {
                       $("#region_form_input").val(ui.item.value);
                       $obj = $(".your_city .selectBlock .optionList");
                       $('<div class="regionsetProgress"></div>').appendTo($obj);

                       $("#other_city").val(ui.item.label);
                       $.getJSON("/ajax/basket/update-region.php",
                           {region_id: ui.item.value},
                           function(json){
                               if (json.message) {
                                   $(".change-bskt-rgn-ntfctn .message").html(json.message);
                                   $(".change-bskt-rgn-ntfctn").css('top', 60 + $(window).scrollTop());
                                   $(".change-bskt-rgn-ntfctn").show();
                               }else {
                                   $("#region_form").submit();
                               }
                           }
                       );
                   } else {
                       $(".your_city .optionList").hide();
                   }
                   return false;
               }
           });
       });
    </script>

