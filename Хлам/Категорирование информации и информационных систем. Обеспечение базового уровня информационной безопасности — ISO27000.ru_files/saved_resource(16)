            socialEventsUrl: "https://lk.wildberries.ru/newsfeed/events",
            enableProfilePageForEveryone: true,            
        };
        $.views.converters({
            urlForGood: function(cod1S, absolut) {
                    return (absolut ? 'https://www.wildberries.ru' : '') + '/catalog/' + cod1S + '/detail.aspx';
                },
            urlForStatic: function(path, version) {
                    var url = '//static.wbstatic.net' + path;
                    return url;
                },
            urlForSaleIcon: function(iconId) {
                    return '//images.wbstatic.net' + '/promotions/wide/' + iconId + '.png';
                },
            urlForJsTmpl: function(name) {
                    return '/jst/name?v=48491'.replace('name', name);
                },
            toLower: function(str) {
                    return str.toLowerCase();
            },
            dateFromTimestamp: function (timestamp, format) {
                return new Date(timestamp).format(format || 'dd.MM.yyyy HH:mm');
            }
            });
        $.views.helpers({
            wbSettings: wb.settings,
            urlSignIn: function (returnUrl) {
                    return "https://security.wildberries.ru/login?returnUrl=" + (returnUrl || window.location.href);
                },
            isTestUser: function() {
                return $.inArray(parseInt(wb.marketingUser.uid || 0), [9264,6585795,6585802,4494954,6077600,6587177,6587186,6585843,6585863,6585888]) > -1;
            }
