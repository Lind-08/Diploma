$(function() {
    
        $('.main-banners').find('img').lazyload({
            effect: "fadeIn",
            threshold: 700,
            failurelimit: 4,
            onWindowLoad: function() { $(window).trigger("scroll"); }
        });

        if (wb.stat) {
            var request = wb.stat.request;
            var params = [];

            $.each($("div#newsSmallBanners a>img[data-id]"), function() {
                params.push(request.wrapParam("BId", $(this).attr("data-id")));
                $(this).click(function() {
                    request.sendEvent("BCE", [wb.stat.request.wrapParam("BId", $(this).attr('data-id'))]);
                });
            });

            if (params.length > 0) {
                wb.stat.request.sendEvent("BSE", params);
            }
        }
    

    
        wb.home();
    

    
        if (wb.stat) {
            var bigBanner = $("#big-banner");
            if (bigBanner.length) {
                var id = bigBanner.attr("data-id");
                wb.stat.request.sendEvent("BSE", [wb.stat.request.wrapParam("BId", id)]);
                bigBanner.find("a").on("click", function() {
                    wb.stat.request.sendEvent("BCE", [wb.stat.request.wrapParam("BId", id)]);
                });
            }
        }
    

    
        $('.brands-group').find('img').lazyload({ effect: "fadeIn", threshold: 700, failurelimit: 4, event: 'scroll' });
    

});
$(window).load(function() {
    
        if (!wb.settings.isMobileDevice && !wb.settings.disableBackground) {
            var $html = $('html'),
                $body = $('body'),
                leftBackground = '',
                rightBackground = '',
                backgroundColor = '';
            
            if (leftBackground)
                $html.css('background', leftBackground);
            if (rightBackground)
                $body.css('background', rightBackground);
            if (backgroundColor)
                $html.css('background-color', backgroundColor);
        }
    

        
            var $novelties = $(".novelties").each(function(idx, el) {
                var $this = $(el),
                    vert = $this.data('position-vertical');
                $this.jCarouselLite({
                    btnNext: $this.siblings('.next'),
                    btnPrev: $this.siblings('.prev'),
                    vertical: vert,
                    lazyImg: true,
                    responsive: !vert,
                    preventTouchWindowScroll: false,
                    swipeClick: true,
                    swipeThresholds: {
                        x: 20,
                        y: 80,
                        time: 0,
                        kMinSwipe: 0.2
                    },
                    visible: $this.attr('data-visible-count')
                });
            });
            
        

        
        var $recentLooks = $("#userLooksCarousel");
        $recentLooks.jCarouselLite({
