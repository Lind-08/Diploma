                { minWidth: 1500, maxWidth: +Infinity, imgSize: "c283x377" }],
            isMobileDevice: (/android|webos|iphone|ipad|ipod|blackberry|iemobile|opera mini/i.test(navigator.userAgent.toLowerCase())),
            isTouch: 'ontouchend' in document,
            newsEnabled: "False",
            newsLikes: 'False',
            showEvents: "True",
            pooWorkload: true,
            geo: {
                cookie: '__wbl',
                phoneContactDict: {"0":"8 (800) 775 55 05","77":"8 (495) 775 55 05","50":"8 (800) 775 55 05","47":"8 (495) 775 55 05","-1":"8 (800) 775 55 05","78":"8 (495) 775 55 05","66":"8 (343) 236 62 35"}
                },
