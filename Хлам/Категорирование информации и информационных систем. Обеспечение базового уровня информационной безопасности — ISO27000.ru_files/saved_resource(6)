                <script>
    $(media_markt).unbind('basket.addItem').bind('basket.addItem', function(e, item){
        if (typeof item != "undefined"){
            var data = {
                "pageType":8,
                "currentProduct":{
                    "id": item.article,
                    "name": item.full_item.title,
                    "price": item.price
                }
            };
            try {APRT_SEND(data);} catch(e){}
        }
    });
    $(media_markt).unbind('basket.removeItem').bind('basket.removeItem', function(e, item){
        if (typeof item != "undefined"){
            var data = {
                "pageType":9,
                "currentProduct":{
                    "id": item.article,
                    "name": item.full_item.title,
                    "price": item.price
                }
            };
            try {APRT_SEND(data);} catch(e){}
        }
    });
</script>
