                <script>
/**
 * Функция для сбора статистики (GA) операций добавления/удаления товаров с корзины
 * @param e
 * @param item Информация об удаляемом продукте
 * @param eventCategory Значения для разных типов страниц с корзиной [BASKET|HEADER_BASKET|HOMEPAGE|FAVORITE...]
 * @param eventAction Действие пользователя (удаление/добавление товара) [add|remove]
 **/
function ecChangeCartHandler(e, item, eventCategory, eventAction) {
    
    var eventLabel = eventAction === 'add' ? 'addToCart' : 'removeFromCart',
        sendEventAction = eventAction === 'add' ? 'click' : 'remove',
        items = [];

    if (!Array.isArray(item)) {
        items.push(item);
    } else {
        items = item;
    }

//    console.debug('Pixel ID: %s, event: ec.changeCart', 189);
//    console.debug('ga("send", "event", "' + eventCategory + '", "' + sendEventAction + '", "' + eventLabel + '")');

    items.forEach(function (item) {

        var count = item.count || 1;

        for (var i = 0; i < count; i++) {

            var productData = {
                "id": item.article,
                "name": item.title,
                "price": item.price,
                "brand": item.brand,
                "category": item.category,
                "variant": item.variant || 'not specified',
                "position": item.position || 1,
                "quantity": item.quantity || 1
            };

//            console.debug(productData);

            ga("ec:addProduct", productData);
        }
    });

    ga("create", "UA-32405148-1");
    ga("require", "ec");
    ga("ec:setAction", eventAction);
    ga("send", "event", eventCategory, sendEventAction, eventLabel);
    ga("send", "event", "BASKET", "remove", "removeFromCart");
}

$(document).off('ec.changeCart').on('ec.changeCart', ecChangeCartHandler); // слушатель события операций с товарами в корзине
</script>
                <!-- - - - - - - - - - PXL - default_in_body END - - - - - - - - - -->
