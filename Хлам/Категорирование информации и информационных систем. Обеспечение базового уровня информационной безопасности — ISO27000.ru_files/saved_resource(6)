            $(media_markt).on('basket.addItems', function(e, items){console.log(items);
                var goods = [];
                for (item in items){
                    goods.push(
                        {
                            "id": items[item].article,
                            "name": items[item].full_item.title,
                            "quantity": items[item].count,
                            "price":items[item].price
                        }
                    );
                }
                var data = {
                    "currency": "RUB",
                    "goods": goods
                };
                yaCounter8074996.ecommerceAdd(data);
            });
            $(media_markt).on('basket.removeItems', function(e, items){
                var goods = [];
                for (item in items){
                    goods.push(
                        {
                            "id": items[item].article,
                            "name": items[item].full_item.title,
                            "quantity": items[item].count,
                            "price":items[item].price
                        }
                    );
                }
                var data = {
                    "currency": "RUB",
                    "goods": goods
                };
                yaCounter8074996.ecommerceRemove(data);
            });

            $('.spelling-mistake [name="errorText"]').on('change', function(){
