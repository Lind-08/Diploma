                <script>

var ecChangeCartDataAttributesToItem = function (data) {

    var color = 'not specified';

    if (data.color && data.color.color_name) {
        color = data.color.color_name;
    } else if (typeof data.color === "string") {
        color = data.color;
    }

    return {
        article: data.article,
        title: data.name,
        price: data.price,
        brand: data.brand || '',
        category: data.category && data.category.title ? data.category.title : '',
        variant: color,
        position: data.position,
        quantity: data.quantity || 1,
        count: data.count || 1
    }
};

$(document).on('renderpage.finish', function () {

//    console.info('renderpage.finish:ec.changeCart');

    /**
     * Основная корзина
     */

    // уменьшение количества основного товара (Корзина) [-][2][+]
    $(document).off('click.ga', '#basket-list li.li-item .item > .right .minus').on('click.ga', '#basket-list li.li-item .item > .right .minus', function () {

        var items = [],
            data = $(this).closest('li.li-item').data();

        $(this).closest('li.li-item').find('.install-box').each(function (i, el) {

            var item = $(el).data();

            if (item.count == data.count) {

                item = jQuery.extend(item, {
                    count: 1
                });

                items.push(ecChangeCartDataAttributesToItem(item));
            }

        });

        data.count = 1;

        items.push(ecChangeCartDataAttributesToItem(data));

        $(document).trigger('ec.changeCart', [items, 'BASKET', 'remove']);
    });

    // [-][2][+] увеличение количества основного товара (Корзина)
    $(document).off('click.ga', '#basket-list li.li-item .item > .right .plus').on('click.ga', '#basket-list li.li-item .item > .right .plus', function () {
        var data = $(this).closest('li.li-item').data();
        data.count = 1;
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'CHECKOUT', 'add']);
    });

    // уменьшение количества установок товара (Корзина)
    $(document).off('click.ga', 'li.li-item .item > .installation .minus').on('click.ga', 'li.li-item .item > .installation .minus', function () {
        var data = $(this).closest('div.install-box').data();
        data = jQuery.extend(data, {
            count: 1
        });
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'BASKET', 'remove']);
    });

    // увеличение количества установок товара (Корзина)
    $(document).off('click.ga', 'li.li-item .item > .installation .plus').on('click.ga', 'li.li-item .item > .installation .plus', function () {
        var data = $(this).closest('div.install-box').data();
        data = jQuery.extend(data, {
            count: 1
        });
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'CHECKOUT', 'add']);
    });

    // клик по чекбоксу установки
    $(document).off('change.ga', '.installation input[type="checkbox"]').on('change.ga', '.installation input[type="checkbox"]', function (e) {
        var data = $(this).closest('div.install-box').data(),
            action = $(this).prop('checked') ? 'add' : 'remove',
            pageCategory = action === 'add' ? 'CHECKOUT' : 'BASKET';

        data = jQuery.extend(data, {
            count: 1,
            quantity: action == 'remove' ? data.count : 1
        });

        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), pageCategory, action]);
    });

    // удаление товара вместе с установками (Корзина)
    $(document).off('click.ga', '#basket-list .right .delete .js-set-basket-count').on('click.ga', '#basket-list .right .delete .js-set-basket-count', function () {

        var items = [],
                data = $(this).closest('li.li-item').data();

        data = jQuery.extend(data, {
            quantity: data.count,
            count: 1
        });

        items.push(ecChangeCartDataAttributesToItem(data));

        $(this).closest('li.li-item').find('.install-box').each(function (i, el) {

            var item = $(el).data();

            item = jQuery.extend(item, {
                quantity: item.count,
                count: 1
            });

            items.push(ecChangeCartDataAttributesToItem(item));

        });

        $(document).trigger('ec.changeCart', [items, 'BASKET', 'remove']);
    });

    /**
     * Страницы
     */

    // добавление товара (Карточка)
    $(document).off('click.ga', '#product-main-box #product-slide-box .js-add-to-basket').on('click.ga', '#product-main-box #product-slide-box .js-add-to-basket', function () {
        var data = $(this).data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'DETAIL VIEW', 'add']);
    });

    // модификации товара (Карточка)
    $('#main-info-modification').find('.js-add-to-basket').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('tr').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(itemAttributes), 'MODIFICATION', 'add']);
    });

    // добавление товара (Избранное)
    $('[data-page-type="FAVORITE"]').off('click.ga', '.favorites-list .js-add-to-basket').on('click.ga', '.favorites-list .js-add-to-basket', function () {
        var data = $(this).closest('div.cart-item').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'FAVORITE', 'add']);
    });

    // кнопка "Положить все товары в корзину"
    $('[data-page-type="FAVORITE"]').find('#favorites-list-all-products-into-basket').off('click.ga').on('click.ga', function () {

        var items = [];

        $('.favorites-list div.cart-item').each(function (index, el) {
            items.push(ecChangeCartDataAttributesToItem($(el).data()));
        });

        $(document).trigger('ec.changeCart', [items, 'FAVORITE', 'add']);
    });

    // добавление товара (Сравнение)
    $(document).off('click.ga', '#compare-table .js-add-to-basket').on('click.ga', '#compare-table .js-add-to-basket', function () {
        var data = $(this).closest('li').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'COMPARE', 'add']);
    });

    // добавление товара (Каталог)
    $('[data-page-type="CATEGORY"]').off('click.ga', '.js-add-to-basket').on('click.ga', '.js-add-to-basket', function () {
        var data = $(this).closest('li').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'CATEGORY', 'add']);
    });

    // добавление товара (Поиск)
    $('[data-page-type="SEARCH"]').off('click.ga', '.js-add-to-basket').on('click.ga', '.js-add-to-basket', function () {
        var data = $(this).closest('li').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'SEARCH', 'add']);
    });

    // добавление товара (Промо)
    $('[data-page-type="PROMO PAGE"]').off('click.ga', '.js-add-to-basket').on('click.ga', '.js-add-to-basket', function () {
        var data = $(this).closest('li').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'PROMO PAGE', 'add']);
    });

    // добавление товар в /promo/view
    $(document).off('click.ga', '.catalog.promo-info .js-add-to-basket').on('click.ga', '.catalog.promo-info .js-add-to-basket', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(itemAttributes), 'PROMO PAGE', 'add']);
    });

    // добавление товара (Просмотренные товары)
    $('[data-page-type="Последние просмотренные"]').off('click.ga', '.js-add-to-basket').on('click.ga', '.js-add-to-basket', function () {
        var data = $(this).closest('li').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'RECENT', 'add']);
    });

    // добавление товара (страница "Брендов")
    $(document).off('click.ga', '.catalog.brand-page .js-add-to-basket').on('click.ga', '.catalog.brand-page .js-add-to-basket', function (e) {
        var data = $(this).closest('li').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'BRANDS', 'add']);
    });

    /**
     * Блоки
     */

    // добавление товара (Также вам может пригодиться)
    $(document).off('click.ga', '.slider-come-in-handy .js-add-to-basket').on('click.ga', '.slider-come-in-handy .js-add-to-basket', function () {
        var data = $(this).closest('li.item').data();
        $(document).trigger('ec.changeCart', [ecChangeCartDataAttributesToItem(data), 'Также вам может пригодиться', 'add']);
    });

    /**
     * Удаление товара (Блок корзины в шапке)
     * При удалении нескольких товаров передавать значение `count` в `quantity` вместо дублирования запросов
     */
    $(document).off('click.ga', '#basket-item .js-set-basket-count').on('click.ga', '#basket-item .js-set-basket-count', function (e) {

        var items = [],
            data = $(this).closest('li').data(),
            serviceItems = (data.serviceItems || []).map(function (serviceItem) {

                serviceItem = ecChangeCartDataAttributesToItem(serviceItem);

                return jQuery.extend(serviceItem, {
                    quantity: serviceItem.count,
                    count: 1
                });
            });

        data = jQuery.extend(data, {
            position: $(this).closest('ul').find('li').index($(this).closest('li')) + 1,
            quantity: data.count,
            count: 1
        });

        items.push(ecChangeCartDataAttributesToItem(data));

        items = items.concat(serviceItems);

        $(document).trigger('ec.changeCart', [items, 'HEADER_BASKET', 'remove']);
    });

    // добавление нескольких товаров в блоке "Аксессуары"
    $(document).off('click.ga', '#Accessories .js-add-collection-to-cart').on('click.ga', '#Accessories .js-add-collection-to-cart', function (e) {

        var items = [],
                productData = $('#product-slide-box').find('.product-buy-form .js-add-to-basket').data();

        $('#Accessories').find('[type="checkbox"]:checked').closest('li').each(function (index, el) {
            items.push(ecChangeCartDataAttributesToItem($(el).data()));
        });

        items.push(ecChangeCartDataAttributesToItem(productData));

        $(document).trigger('ec.changeCart', [items, 'Аксессуары_MM']);
    });

});

</script>
