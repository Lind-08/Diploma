    function initGaTodayItemsHandler() {
        $('#gaTodayItems').find('a').off('click').on('click', function () {
            var gaTodayArticle = $(this).closest('[data-ga-article]').data('ga-article');
            if (gaTodayArticle != undefined && typeof ga != "undefined"){ ga('send', 'event', 'Main page', 'Товар дня', gaTodayArticle);}
        });
    }
    
