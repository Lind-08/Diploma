                <script>

$(document).on('renderpage.finish', function () {

//    console.info('renderpage.finish');
    
    var gaDataAttrsToItem = function (data) {
        return {
            "id": data.article,
            "name": data.name,
            "price": data.price,
            "brand": data.brand,
            "category": data.category.title || '',
            "position": data.position
        };
    };

    /**
     * Сформировать селекторы для навешивания собый из ID блока и роутов где он будет отображаться
     * @param {object} blocks '{"block.recent": [route1..route3]}'
     * @param appendSelector
     * @return {string}
     */
    var ydf = function (blocks, appendSelector) {

        var selectors = [], blockID;

        for (blockID in blocks) {
            blocks[blockID].map(function (route) {
                selectors.push('#block_' + blockID.replace(/\./g, '_') + '_' + route + '_ydf ' + appendSelector);
            });
        }

        return selectors.join(', ');
    };

    /**
     * TODO СТРАНИЦЫ
     */

    // Последние просмотренные
    $('.favorite-catalog-items').find('.img a, .title a').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'RECENT']);
    });

    // Корзина
    $('#basket-list .title a[href]').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(media_markt).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'CHECKOUT']);
    });
    
    // клик по товару в /promo/all
    $(document).off('click.ga', '.promo-all-items .img a, .promo-all-items .title a').on('click.ga', '.promo-all-items .img a, .promo-all-items .title a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'PROMO PAGE']);
    });

    // клик по товару в /promo/info
    $(document).off('click.ga', '.catalog.promo-info .img a, .catalog.promo-info .title a').on('click.ga', '.catalog.promo-info .img a, .catalog.promo-info .title a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'PROMO PAGE']);
    });

    // Каталог
    var selectorCatalog = '[data-page-type="CATEGORY"] .item-col-left a, [data-page-type="CATEGORY"] .item-col-right a[href]:not([href^="#"])';
    $(document).off('click.ga', selectorCatalog).on('click.ga', selectorCatalog, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'CATEGORY']);
    });

    // Каталог "Поиск"
    var selectorSearch = '.item-col-left .img a, .item-col-left .title a';
    $(document).find('.catalog.search').off('click.ga', selectorSearch).on('click.ga', selectorSearch, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'SEARCH']);
    });

    // Каталог "Бренды"
    var selectorBrands = '.item-col-left .img a, .item-col-left .title a';
    $(document).find('.catalog.brand-page').off('click.ga', selectorBrands).on('click.ga', selectorBrands, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'BRANDS']);
    });
    
    // Сравнение
    $('#compare-table').find('ul.items .top').find('a:last').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'COMPARE']);
    });

    // Избранное
    $('.favorites-list .cart-item').find('.img a, .title a').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('.cart-item').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'FAVORITE']);
    });

    // клик по товару в списке "Другие модификации этого товара"
    $('#main-info-modification').find('.text-block a').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('tr').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'MODIFICATION']);
    });

    // корзина в хедере
    $(document).off('click.ga', '#basket-item li a').on('click.ga', '#basket-item li a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        itemAttributes.position = itemAttributes.lineId + 1;
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'HEADER_BASKET']);
    });

    /**
     * TODO БЛОКИ
     */

        // блок "Чаще всего покупают"
    $(document).off('click.ga', '#block_most_often_buying .item > a').on('click.ga', '#block_most_often_buying .item > a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Чаще всего покупают_MM']);
    });

    // блок "Аксессуары"
    $(document).off('click.ga', '#Accessories .text a').on('click.ga', '#Accessories .text a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Аксессуары_MM']);
    });

    // блок "Похожие товары" (в карточке справа, снизу слайдер)
    var blockSimilar = ['#block_similar .item .item-col-left .img a', '#block_similar .item .item-col-left .title a', '#slider-similar-items .item > a'].join(', ');
    $(document).off('click.ga', blockSimilar).on('click.ga', blockSimilar, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Похожие товары_MM']);
    });

    // блок "Поколения"
    var blockGeneration = ['#block_generation .item .item-col-left .img a', '#block_generation .item .item-col-left .title a'].join(', ');
    $(document).off('click.ga', blockGeneration).on('click.ga', blockGeneration, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Поколения_MM']);
    });
    
    // блок "Последние просмотренные"
    $(document).off('click.ga', '#slider-recent-items .item > a').on('click.ga', '#slider-recent-items .item > a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Последние просмотренные_MM']);
    });
    
    // клик по товару в списке "Сейчас покупают"
    $('.block-currently-buy').find('.item > a').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Сейчас покупают_MM']);
    });

    // блок "Фантастиш предложения"
    $(document).off('click.ga', '.home #main-items-slider .item a').on('click.ga', '.home #main-items-slider .item a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Фантастиш предложения_MM']);
    });

    // блок "Также вам может пригодиться"
    $('.slider-come-in-handy').find('a').off('click.ga').on('click.ga', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Также вам может пригодиться_MM']);
    });

    // блок "Предложения недели"
    $(document).off('click.ga', '#block_week_offers .item a').on('click.ga', '#block_week_offers .item a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Предложения недели_MM']);
    });

    // блок "Товары из нашей рекламы"
    $(document).off('click.ga', '#block_our_products_advertise .item a').on('click.ga', '#block_our_products_advertise .item a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Товары из нашей рекламы_MM']);
    });

    // блок "Хиты продаж"
    $(document).off('click.ga', '#block_sales_hits .item a').on('click.ga', '#block_sales_hits .item a', function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Хиты продаж_MM']);
    });

    /**
     * TODO БЛОКИ ЯНДЕКСА
     */

    // блок "Рекомендуем Вам"
    var blockRecommends = ydf({'base.recommends': ['main', '404'], 'basket.list.empty': ['basket', 'basket_done'], 'search.empty': ['search']}, '.item > a');
    $(document).off('click.ga', blockRecommends).on('click.ga', blockRecommends, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Рекомендуем Вам_YDF']);
    });

    // блок "Похожие товары"
    var blockSimilarYDF = ydf({'item.similar.exists': ['product'], 'item.similar.missing': ['product']}, '.item > a');
    $(document).off('click.ga', blockSimilarYDF).on('click.ga', blockSimilarYDF, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Похожие товары_YDF']);
    });

    // блок "Вместе с этими товарами покупают"
    var blockTogother = ydf({'basket.list': ['basket']}, '.item > a');
    $(document).off('click.ga', blockTogother).on('click.ga', blockTogother, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Вместе с этими товарами покупают_YDF']);
    });

    // блок "Рекомендуем в категории"
    var blockRecommendsInCategory = ydf({'base.recommends': ['catalog1', 'catalog2']}, '.item > a');
    $(document).off('click.ga', blockRecommendsInCategory).on('click.ga', blockRecommendsInCategory, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Рекомендуем в категории_YDF']);
    });

    // блок "Покупают прямо сейчас"
    var blockBuyingNow = ydf({'base.buy_now': ['catalog1', 'catalog2']}, '.item > a');
    $(document).off('click.ga', blockBuyingNow).on('click.ga', blockBuyingNow, function (e) {
        var itemAttributes = $(this).closest('li').data();
        $(document).trigger('ec.itemTransitionToCard', [gaDataAttrsToItem(itemAttributes), 'Покупают прямо сейчас_YDF']);
    });
    
});
</script>
