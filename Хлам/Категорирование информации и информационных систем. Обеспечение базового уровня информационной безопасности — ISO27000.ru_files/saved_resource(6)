                <script>

// название события: "ec.itemTransitionToCard"
// параметры: ['товар', 'название страницы']
// пример: $(media_markt).trigger('ec.itemTransitionToCard', [{}, 'FAVORITE']);

/**
 * Сбор данных для GA для события перехода на карточку товара
 * @param e
 * @param item
 * @param eventCategory Значения для разных типов страниц
 */
function ecItemTransitionToCard(e, item, eventCategory) {
    
    var pixelID = 189,
        eventName = 'ec.itemTransitionToCard';
    
    var productData = {
        "id": item.id,
        "name": item.name,
        "price": item.price,
        "brand": item.brand,
        "category": item.category,
        "position": item.position
    };

//    console.debug("Pixel ID: %s, event: '%s'", pixelID, eventName);
//    console.debug(productData);
//    console.debug('ga("send", "event", ' + eventCategory + ', "click", "")');

    ga("create", "UA-32405148-1");
    ga("require", "ec");
    ga("ec:addProduct", productData);
    ga("ec:setAction", "click", {"list": eventCategory});
    ga("send", "event", eventCategory, "click", "");
}

$(media_markt).off('ec.itemTransitionToCard').on('ec.itemTransitionToCard', ecItemTransitionToCard); // слушатель события переход на карточку товара
</script>
