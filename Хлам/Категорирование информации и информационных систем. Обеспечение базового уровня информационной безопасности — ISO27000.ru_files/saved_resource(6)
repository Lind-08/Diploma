    <script type="text/javascript">
        /*
        window.addEventListener('load', function () {
            var l = document.createElement('link');
            l.rel = 'stylesheet';
            l.href = '/assets/css/fonts.css';
            var h = document.getElementsByTagName('head')[0];
            h.parentNode.insertBefore(l, h);
        });
        */
        $(document).on('renderpage.finish', function () {
            $(document).trigger("renderpage.loadexternals");
        });

        $(document).on('renderpage.loadexternals', function () {

            $('#pxls-block').find('.pxl-iter').each(function(){
                $('body').append($(Base64.decode($(this).text())));
            });

            $('#pxls-block').remove();
        });

        $(document).ajaxComplete(function() {
            setTimeout(function() {
                $(window).lazyLoadXT({visibleOnly: false, checkDuplicates: true});
            }, 50);
        });

        $(window).load(function() {
            $(window).lazyLoadXT({visibleOnly: true});
        });

        // lazy-loading of images in scroll viewport
        $('.lazy-load-scroll-container').on('scroll', function(){
            // trigger event, which will check what elements are visible in current viewport
            $(document).trigger('scroll');
        });
    </script>

            <script type="text/javascript">
            // load pixels via ajax after load whole page if need
            window.addEventListener('load', function (){
                if (window.performance) {
                    ga('send', 'timing', 'Page Loading', 'Before Pixels', performance.now());
                }

                var data = {
                    _token: $('meta[name="csrf-token"]').attr('content'),
                    'request-key': $('meta[name="request-hash"]').attr('content')
                };

                $.post('/ajax/pixels', data, function (response) {
                    try {
                        for (var i in response) {
                            $('body').append($(response[i].html));
                        }
                    } catch (e) {
                        loggingAjaxDefectivePixel(e.message, response[i].html);
                    }
                });
            });
        </script>
    </body>
