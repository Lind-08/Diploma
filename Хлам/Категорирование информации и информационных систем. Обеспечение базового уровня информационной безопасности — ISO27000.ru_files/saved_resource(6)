    <script src="https://www.mediamarkt.ru/assets/js/header.js"></script>

    <script src="https://www.mediamarkt.ru/assets/js/user.js"></script>

    <script src="https://www.mediamarkt.ru/assets/js/basket/basket3.js"></script>

    <script src="https://www.mediamarkt.ru/assets/js/scripts.js"></script>

    <script src="https://www.mediamarkt.ru/assets/js/layouts/master.js"></script>

