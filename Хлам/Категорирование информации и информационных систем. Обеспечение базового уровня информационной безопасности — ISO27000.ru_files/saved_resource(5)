<script type="text/javascript">
	
	var rt_curdate=new Date();
	var rt_timeout=60000;
	var rt_reshow=1;
	function rt_send_by_type(type, rt_popup_group){
		(function (d, w) {
			if(type==1 || type==2){
				var action_title=type==1 ? "close_cross" : "close_background";
				dataLayer.push({
					'GAEventCategory': 'Site Track',
					'GAEventAction': 'pop300_'+action_title+'_a',
					'GAEventLabel': '',
					'event': 'GAEvent'
				});
			}
			rt_data_ac={adv_id: "1", event: "action", type: type, rt_popup_group: rt_popup_group};
			var ts = d.createElement("script"); ts.type = "text/javascript"; ts.async = true;
			ts.src = (d.location.protocol == "https:" ? "https:" : "http:") + "//rt.biglion.ru/rt_ac.js";
			var f = function () {var s = d.getElementsByTagName("script")[0]; s.parentNode.insertBefore(ts, s);};
			if (w.opera == "[object Opera]") { d.addEventListener("DOMContentLoaded", f, false); } else { f(); }
		})(document, window);
	}
	if(!$.cookie("client_id") && 'a'!='a' && (!$.cookie("rt_sess_start") || rt_curdate.getTime()-$.cookie("rt_sess_start")<rt_timeout)){
		var rt_data_ir={adv_id: "1", event: "is_registered"};
		var rt_data_response_ir;
		var rt_data_ac={adv_id: "1", event: "action"};
		(function (d, w) {
			var ts = d.createElement("script"); ts.type = "text/javascript"; ts.async = true;
			ts.src = (d.location.protocol == "https:" ? "https:" : "http:") + "//rt.biglion.ru/rt_ir.js";
			var f = function () {var s = d.getElementsByTagName("script")[0]; s.parentNode.insertBefore(ts, s);};
			if (w.opera == "[object Opera]") { d.addEventListener("DOMContentLoaded", f, false); } else { f(); }
		})(document, window);
		$(document).ready(function(){
			if(!$.cookie("rt_sess_start")){
				var domen = document.location.host.match(/[^\.]+$/);
				$.cookie("rt_sess_start", rt_curdate.getTime(), {expires: rt_reshow, path: "/", domain: ".biglion." + domen});
			}
			window.setTimeout(function(){
				if($('#biglion_shadow').css('display')=="none" && !rt_data_response_ir.is_registered && window.location.href.indexOf('registration') == -1){
                    $(".popup_reg3001").dialog({
						modal: true,
						resizable: false,
						height: 508,
						width: 466,
						draggable: false,
						closeText: "Закрыть"
					});
                    $(".ui-widget-header").addClass('opacity_popup');
                    $("body .ui-widget-content").addClass('opacity_popup');
                    
                    $(".ui-dialog-titlebar-close").addClass('gtm_tr_pop300_close_a');
                    $(".ui-widget-overlay").addClass('gtm_tr_pop300_backgroud_a');
                    
					$(".ui-dialog-titlebar-close").click(function(){rt_send_by_type(1, 'a');});
					$(".ui-widget-overlay").click(function(){rt_send_by_type(2, 'a');});
					$(".rt_auth").click(function(){rt_send_by_type(3, 'a');});
					$(".rt_reg").click(function(){rt_send_by_type(4, 'a');});
					dataLayer.push({
						'GAEventCategory': 'Site Track',
						'GAEventAction': 'pop300_open_a',
						'GAEventLabel': '',
						'event': 'GAEvent'
					});
					$.cookie("rt_popup_group", 'a', {expires: 365, path: "/", domain: ".biglion." + domen}); //ab
				}
			}, rt_timeout-(rt_curdate.getTime()-$.cookie("rt_sess_start")));

		});
	}
	
</script>

