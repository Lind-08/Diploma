    <script>
        $(document).ready(function() {

            //ARROW and Header collapse
            $(window).scroll(function(){
                window.onScrollTmp();
            });

            $('.citylist-hide').on( 'click', function() {
                if ($('body').hasClass('mobile')) {
                    $('#header0').css('top', window.posTop);
                } else {
                    if ( $(window).scrollTop() > 0 ) {
                        $('#header0').css('top', '-502px');
                        $('.top_main_menu_block0').css('top', '49px');
                        $('.main_logo_top').css('top', '19px').find('span').css('top', '3px');
                    } else {
                        $('#header0').css('top', window.posTop);
                        $('.top_main_menu_block0').css('top', window.posTop2);
                    }
                }
            });

            $('#scrollNavArrow').bind('mouseover', function () {
                $(this).css('opacity',1);
            });

            $('#scrollNavArrow').bind('mouseout', function () {
                $(this).css('opacity',0.8);
            });

            $('#scrollNavArrow').live('click', function () {
                $('html, body').animate({scrollTop:0}, 'fast');
                return false;
            });


            var slideTopHorMenu = function(){
                if ($('body').hasClass('mobile')) {
                    if( parseInt($('#header0').css('top')) < 0 ){
                        $('#header0').css('top', '0');
                        $('.main_logo_top').css('top', '-3px').find('span').css('top', '0');
                    } else {
                        $('#header0').css('top', window.posTop);

                    }
                } else {
                    if( parseInt($('#header0').css('top')) < 0 ){
                        $('#header0').css('top', '0');
                        $('.top_main_menu_block0').css('top', '551px');
                        $('.main_logo_top').css('top', '-3px').find('span').css('top', '0');
                    } else {
                        if ( $(window).scrollTop() > 0 ) {
                            $('#header0').css('top', '-502px');
                            $('.top_main_menu_block0').css('top', '49px');
                            $('.main_logo_top').css('top', '22px').find('span').css('top', '5px');
                        } else {
                            $('#header0').css('top', window.posTop);
                            $('.top_main_menu_block0').css('top', window.posTop2);
                        }
                    }
                }
            }


            window.posTop = '-471px';
            window.posTop2 = '80px';
            window.posPlus = 0;

            window.initPage = function () {

                $('#choose_city_block li').live('click', function(){
                    var thisLi = $(this).html();
                    $('#choose_city_btn b').html('').html(thisLi);
                });

                //MAIN PAGE -->   TOP BLOCK -->   OPEN CITIES BLOCK

                var
                        top = 471,
                        topHorMenuTop = parseInt($('#topHorMenu').css('top')),
                        contentTop = padCont;

                var padCont = $('#header0').outerHeight(true) - $('#citylist-mbl').outerHeight(true) + $('.top_main_menu_block').outerHeight(true) + $('.top_main_submenu').outerHeight(true)+window.posPlus;

                $('#content0').css('padding-top', padCont);
                $('body.mobile #content0').css('padding-top', '150px');

                clickToggleBtn(
                        'div#choose_city_btn, #choose_city_close_btn, #choose_city_block li',
                        'div#choose_city_block0',
                        '',
                        '0px -16px',
                        '0px 1px',
                        function(){slideTopHorMenu()}, null, {click:true, touch:true}
                );

            };
            window.initPage();

            window.onScrollTmp = function(){

                if (!$('body').hasClass('mobile')) {
                    if($(window).scrollTop() > 37) {
                        $('#scrollNavArrow').show();
                    } else {
                        $('#scrollNavArrow').hide();
                    }
                    if ( parseInt($('#header0').css('top')) == 0 ) {
                    } else {
                        if ($(window).scrollTop() > 37) {
                            $('#header0').css('top', '-502px');
                            $('.top_main_menu_block0').css('top', '49px');
                            $('.main_logo_top').css('top', '22px').find('span').css('top', '5px');
                            $('#choose_city_btn').css('top', '48px');
                            $('.delivery-header').css('height', '40px');
                        } else {
                            $('#header0').css('top', window.posTop);
                            $('.top_main_menu_block0').css('top', window.posTop2);
                            $('.main_logo_top').css('top', '-3px').find('span').css('top', '0');
                            $('#choose_city_btn').css('top', '30px');
                            $('.delivery-header').css('height', '24px');
                        }
                    }
                }

            };

        });
    </script>
