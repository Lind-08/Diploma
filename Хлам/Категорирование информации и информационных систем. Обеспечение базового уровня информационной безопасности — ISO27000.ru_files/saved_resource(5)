<script type="text/javascript">
    var MAILRU_APPID = '651135';
    var FACEBOOK_APPID = '439599466123548';
    var VK_APPID = '2688775';
    var PP_APPID = '';
    var PP_URL = '';
    var full_url = 'http://www.biglion.ru/auth/';
    var full_host = 'http://www.biglion.ru';
    window.LINKTOemail = '';
    window.LINKTO = '';


    var providers = {"vk":"ВКонтакте","facebook":"Facebook","mailru":"Mail.ru","twitter":"Twitter","pp":"PayPal"};

    function PPLogin() {
        $('#oauthLoader').show();
        $('div.error').html('');
        var url = "http://www.biglion.ru/auth/?paypal_url=1";
        window.LINKTO = 'paypal';
        oauth_window = window.open(url, "PayPal", "menubar=yes, addressbar=0, width=500, height=700");
        oauth_window.focus();
        return false;
    }

    function MAILRULogin() {
        $('#oauthLoader').show();
        $('div.error').html('');
        var url = 'https://connect.mail.ru/oauth/authorize?client_id='+MAILRU_APPID+'&scope=photos guestbook stream messages events&response_type=code&redirect_uri='+encodeURIComponent(full_url+'?oauth=mailru')+'&host='+full_host;
        window.LINKTO = 'mailru';
        oauth_window = window.open(url, "Mail", "menubar=yes, addressbar=0, width=550, height=550");
        oauth_window.focus();
    }

    function FACEBOOKLogin() {
        $('#oauthLoader').show();
        $('div.error').html('');
        var url = "https://www.facebook.com/dialog/oauth?client_id="+FACEBOOK_APPID+"&redirect_uri="+encodeURIComponent(full_url+'?oauth=facebook')+"&scope=offline_access,user_about_me,user_activities,user_birthday,user_checkins,user_events,user_groups,user_likes,user_notes,user_photos,email,read_stream,create_event,publish_checkins,publish_stream,sms";
        window.LINKTO = 'facebook';
        oauth_window = window.open(url, "Facebook", "menubar=yes, addressbar=0, width=900, height=600");
        oauth_window.focus();
    }


    function VKLogin() {
        $('#oauthLoader').show();
        $('div.error').html('');
        var url = "http://api.vkontakte.ru/oauth/authorize?client_id="+VK_APPID+"&scope=notify,friends,photos,wall,groups,offline&redirect_uri="+encodeURIComponent(full_url+'?oauth=vk')+"&display=popup&response_type=code";
        window.LINKTO = 'vk';
        oauth_window = window.open(url, "Vkontakte", "menubar=yes, addressbar=0, width=500, height=400");
        oauth_window.focus();
    }

    function TWITTERLogin() {
        $('#oauthLoader').show();
        $('div.error').html('');
        var url = "http://www.biglion.ru/auth/?twitter_url=1";
        window.LINKTO = 'twitter';
        oauth_window = window.open(url, "Twitter", "menubar=yes, addressbar=0, width=500, height=700");
        oauth_window.focus();
        return false;
    }

