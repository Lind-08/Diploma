            this.getAliasesListByPlacementName = function(placementName) {
                var me = this;
                var resultList = [];
                var dictionary = me.placementsDictionary;
                for (var item in dictionary) {
                    if (dictionary.hasOwnProperty(item)) {
                        if (dictionary[item].config.placementName == placementName) {
                            resultList.push(dictionary[item].config.alias);
                        }
                    }
                }
                return resultList;
            };

            this.transformPlacementListByNameToMapByAliases = function(placementList) {
                var me = this;
                var resultMap = {};
                for (var i = 0; i < placementList.length; i++) {
                    var placement = placementList[i];
                    var placementName = placement['placement_name'];
                    var aliasesList = me.getAliasesListByPlacementName(placementName);
                    for (var j = 0; j < aliasesList.length; j++) {
                        resultMap[aliasesList[j]] = placement;
                    }
                }
                return resultMap;
            };

            this.initShowMore = function(section) {
                var el = section.find('#rich_relevance_super_prices,#rich_relevance_novely');
                var id = el.prop('id');
                var $container = $('#' + id + '_container');
                var boxheight = $container.data('boxheight') || '240';
                var counter = $container.data('counter') || 1;
                var moreButton = $('#' + id + '_nav .js-more-button');
                moreButton.not(':first').remove();
                moreButton.first().on('click', function(){
                    $container.removeClass('h-box_h' + boxheight).removeClass('h-box_h2' + boxheight + '_x' + counter);
                    counter++;
                    $container.addClass('h-box_h' + boxheight + '_x' + counter);
                    if (counter >= el.data('pages-num')) {
                        $(this).parent().remove();
                    }
                    return false;
                });
            };

            this.initCarousel = function(section, controlIndex, scrollAfterNumber, maxVisibleNumber, circular){
                var carousel = section.find('.jcarousel');
                var containerClassName = 'jcarousel-wrap' + controlIndex;
                var wrap = section.find('.b-products__body-wrap');
                section.addClass(containerClassName);

                if (carousel.find('li').length > scrollAfterNumber) {
                    var prev = $('<div class="b-products__prev b-ico-round b-ico-round_theme_normal b-ico-round_lg fa fa-angle-left g-hidden" title="Предыдущий"></div>');
                    var next = $('<div class="b-products__next b-ico-round b-ico-round_theme_normal b-ico-round_lg fa fa-angle-right g-hidden" title="Следующий"></div>');

                    prev.add(next).removeClass('g-hidden').addClass('g-clickable').appendTo(wrap);
                    carousel.jCarouselLite({
                        scroll: 1,
                        visible: maxVisibleNumber,
                        btnPrev: '.' + containerClassName + ' .b-products__prev:first',
                        btnNext: '.' + containerClassName + ' .b-products__next:first',
                        circular: circular
                    });
                }
            };

            this.destroyCarousel = function(section){
                section.find('.b-products__prev, .b-products__next').remove();
            };

            this.initTabbedCarousel = function(section, controlIndex, scrollAfterNumber, maxVisibleNumber, circular, rrGoodsLinks) {
                var me = this;
                var menu = section.find('.b-products__header .btn-group');
                var content = section.find('.b-products__list');
                var preloader = $('<i class="b-ico b-ico_preloader_md"></i>');
                var busy = false;

                menu.find('.btn').on('click', function(e){
                    if (busy) {
                        e.preventDefault();
                    }

                    var link = $(this);
                    var href = link.attr('href');
                    var dataGoodId = link.attr('data-good-id');
                    var dataCategoryId = link.attr('data-category-id');
                    var dataGoodIds = link.attr('data-good-ids');

                    busy = true;
                    content.empty().append(preloader);
                    $.ajax({
                        url: '/satelliteGoodsListBlock',
                        type: 'POST',
                        dataType: 'html',
                        data: {goodId: dataGoodId, categoryId: dataCategoryId, goodIds: dataGoodIds},
                        success : function(data) {
                            var el = $(data);
                            content.empty();
                            me.destroyCarousel(section);
                            el.each(function(i){
                                content.append($(this));
                            });
                            if (rrGoodsLinks) {
                                var goodIds = rrGoodsLinks['goodIds'];
                                var goodLinks = rrGoodsLinks['goodLinks'];
                                for (var i = 0; i < goodIds.length; i++) {
                                    var $linkImage = content.find('a#linkImage' + goodIds[i]);
                                    var $linkText = content.find('a#linkText' + goodIds[i]);
                                    if ($linkImage.length) {
                                        $linkImage.attr('href', goodLinks[i]);
                                    }
                                    if ($linkText.length) {
                                        $linkText.attr('href', goodLinks[i]);
                                    }
                                }
                            }
                            me.initCarousel(section, controlIndex, scrollAfterNumber, maxVisibleNumber, circular);
                            busy = false;
                        }
                    });

                    return e.preventDefault();
                });

                me.initCarousel(section, controlIndex, scrollAfterNumber, maxVisibleNumber, circular);
            };

            this.showNativeTopGoodsListBlock = function (placementConfig) {
                var $section = $(placementConfig.blockSelector);
                if ($section.length == 1) {
                    $section.show();
                }
            };

            this.showNativeSuperGoodsListBlock = function (placementConfig) {
                var me = this;
                var $section = $(placementConfig.blockSelector);
                if ($section.length == 1) {
                    $section.show();
                    me.initShowMore($section);
                }
            };

            this.showNativeSimilarGoodsListBlock = function (placementConfig) {
                var me = this;
                var $section = $(placementConfig.blockSelector);
                if ($section.length == 1) {
                    $section.show();
                    me.initCarousel($section, placementConfig.carouselControlIndex, placementConfig.carouselScrollAfterNumber, placementConfig.carouselMaxVisibleNumber, placementConfig.carouselCircular);
                }
            };

            this.showNativeSatelliteCategoryGoodsListBlock = function (placementConfig) {
                var me = this;
                var $section = $(placementConfig.blockSelector);
                if ($section.length == 1) {
                    $section.show();
                    me.initTabbedCarousel($section, placementConfig.carouselControlIndex, placementConfig.carouselScrollAfterNumber, placementConfig.carouselMaxVisibleNumber, placementConfig.carouselCircular);
                }
            };

            this.renderTopGoodsListBlock = function (placementConfig) {
                var me = this;
                var placementInfo = me.rrPlacementsInfo[placementConfig.alias];
                if (placementInfo) {
                    var blockSelector = placementConfig.blockSelector;
                    var $section = $(blockSelector);
                    if ($section.length == 1) {
                        if (!$section.is(":visible")) {
                            if (placementInfo['error']) {
                                me.showNativeTopGoodsListBlock(placementConfig);
                            } else {
                                $section.html(placementInfo['data']);
                                var goodIds = placementInfo['goodIds'];
                                var goodLinks = placementInfo['goodLinks'];
                                for (var i = 0; i < goodIds.length; i++) {
                                    var $linkImage = $section.find('a#linkImage' + goodIds[i]);
                                    var $linkText = $section.find('a#linkText' + goodIds[i]);
                                    if ($linkImage.length) {
                                        $linkImage.attr('href', goodLinks[i]);
                                    }
                                    if ($linkText.length) {
                                        $linkText.attr('href', goodLinks[i]);
                                    }
                                }
                                $section.show();
                            }
                        }
                    }
                }
            };

            this.renderSuperGoodsListBlock = function (placementConfig) {
                var me = this;
                var placementInfo = me.rrPlacementsInfo[placementConfig.alias];
                if (placementInfo) {
                    var blockSelector = placementConfig.blockSelector;
                    var $section = $(blockSelector);
                    if ($section.length == 1) {
                        if (!$section.is(":visible")) {
                            if (placementInfo['error']) {
                                me.showNativeSuperGoodsListBlock(placementConfig);
                            } else {
                                $section.html(placementInfo['data']);
                                var goodIds = placementInfo['goodIds'];
                                var goodLinks = placementInfo['goodLinks'];
                                for (var i = 0; i < goodIds.length; i++) {
                                    var $linkImage = $section.find('a#linkImage' + goodIds[i]);
                                    var $linkText = $section.find('a#linkText' + goodIds[i]);
                                    if ($linkImage.length) {
                                        $linkImage.attr('href', goodLinks[i]);
                                    }
                                    if ($linkText.length) {
                                        $linkText.attr('href', goodLinks[i]);
                                    }
                                }
                                $section.show();
                                me.initShowMore($section);
                            }
                        }
                    }
                }
            };

            this.renderSimilarGoodsListBlock = function (placementConfig) {
                var me = this;
                var placementInfo = me.rrPlacementsInfo[placementConfig.alias];
                if (placementInfo) {
                    var blockSelector = placementConfig.blockSelector;
                    var $section = $(blockSelector);
                    if ($section.length == 1) {
                        if (!$section.is(":visible")) {
                            if (placementInfo['error']) {
                                me.showNativeSimilarGoodsListBlock(placementConfig);
                            } else {
                                $section.html(placementInfo['data']);
                                var goodIds = placementInfo['goodIds'];
                                var goodLinks = placementInfo['goodLinks'];
                                for (var i = 0; i < goodIds.length; i++) {
                                    var $linkImage = $section.find('a#linkImage' + goodIds[i]);
                                    var $linkText = $section.find('a#linkText' + goodIds[i]);
                                    if ($linkImage.length) {
                                        $linkImage.attr('href', goodLinks[i]);
                                    }
                                    if ($linkText.length) {
                                        $linkText.attr('href', goodLinks[i]);
                                    }
                                }
                                //header text update
                                if (placementConfig.alias == 'itemPageAlternativeNotAvailable') {
                                    var customHeaderText = $section.attr('data-availability-status-text');
                                    $section.find('span#rich_relevance_custom_header_text').html(customHeaderText);
                                }

                                $section.show();
                                me.initCarousel($section, placementConfig.carouselControlIndex, placementConfig.carouselScrollAfterNumber, placementConfig.carouselMaxVisibleNumber, placementConfig.carouselCircular);
                            }
                        }
                    }
                }
            };

            this.renderSatelliteCategoryGoodsListBlock = function (placementConfig) {
                var me = this;
                var placementInfo = me.rrPlacementsInfo[placementConfig.alias];
                if (placementInfo) {
                    var blockSelector = placementConfig.blockSelector;
                    var $section = $(blockSelector);
                    if ($section.length == 1) {
                        if (!$section.is(":visible")) {
                            if (placementInfo['error']) {
                                me.showNativeSatelliteCategoryGoodsListBlock(placementConfig);
                            } else {
                                $section.html(placementInfo['data']);
                                var goodIds = placementInfo['goodIds'];
                                var goodLinks = placementInfo['goodLinks'];
                                for (var i = 0; i < goodIds.length; i++) {
                                    var $linkImage = $section.find('a#linkImage' + goodIds[i]);
                                    var $linkText = $section.find('a#linkText' + goodIds[i]);
                                    if ($linkImage.length) {
                                        $linkImage.attr('href', goodLinks[i]);
                                    }
                                    if ($linkText.length) {
                                        $linkText.attr('href', goodLinks[i]);
                                    }
                                }

                                $section.show();
                                me.initTabbedCarousel($section, placementConfig.carouselControlIndex, placementConfig.carouselScrollAfterNumber, placementConfig.carouselMaxVisibleNumber, placementConfig.carouselCircular, {goodIds: goodIds, goodLinks: goodLinks});
                            }
                        }
                    }
                }
            };

            this. getTopGoodsListBlock = function (placementMap, placementConfig) {
                var me = this;
                var placementAlias = placementConfig.alias;
                var placement = placementMap[placementAlias];
                if (placement) {
                    var stratMessage = placement['strat_message'];
                    var goodIds = [];
                    var goodLinks = [];
                    for (var i = 0; i < placement['recs'].length; i++) {
                        goodIds.push(placement['recs'][i]['pid']);
                        goodLinks.push(placement['recs'][i]['ct_url']);
                    }
                    $.ajax({
                        url: '/topGoodsListBlock',
                        type: 'POST',
                        dataType: 'html',
                        data: {headerText: ((stratMessage == null) ? "" : stratMessage), goodIds: goodIds.join("\t")}
                    }).success(function (data) {
                        me.rrPlacementsInfo[placementAlias] = {
                            data: data,
                            goodIds: goodIds,
                            goodLinks: goodLinks
                        };
                        me.renderTopGoodsListBlock(placementConfig);
                    }).fail(function (data) {
                        console.log('Error /topGoodsListBlock');
                        me.rrPlacementsInfo[placementAlias] = {
                            error: true
                        };
                        me.renderTopGoodsListBlock(placementConfig);
                    });
                } else {
                    me.rrPlacementsInfo[placementAlias] = {
                        error: true
                    };
                    me.renderTopGoodsListBlock(placementConfig);
                }
            };

            this.getSuperGoodsListBlock = function (placementMap, placementConfig) {
                var me = this;
                var placementAlias = placementConfig.alias;
                var placement = placementMap[placementAlias];
                if (placement) {
                    var stratMessage = placement['strat_message'];
                    var goodIds = [];
                    var goodLinks = [];
                    for (var i = 0; i < placement['recs'].length; i++) {
                        goodIds.push(placement['recs'][i]['pid']);
                        goodLinks.push(placement['recs'][i]['ct_url']);
                    }
                    $.ajax({
                        url: '/superGoodsListBlock',
                        type: 'POST',
                        dataType: 'html',
                        data: {headerText: ((stratMessage == null) ? "" : stratMessage), goodIds: goodIds.join("\t"), pageName: placementConfig.pageName, type: placementConfig.type}
                    }).success(function (data) {
                        me.rrPlacementsInfo[placementAlias] = {
                            data: data,
                            goodIds: goodIds,
                            goodLinks: goodLinks
                        };
                        me.renderSuperGoodsListBlock(placementConfig);
                    }).fail(function (data) {
                        console.log('Error /superGoodsListBlock');
                        me.rrPlacementsInfo[placementAlias] = {
                            error: true
                        };
                        me.renderSuperGoodsListBlock(placementConfig);
                    });
                } else {
                    me.rrPlacementsInfo[placementAlias] = {
                        error: true
                    };
                    me.renderSuperGoodsListBlock(placementConfig);
                }
            };

            this.getSimilarGoodsListBlock = function (placementMap, placementConfig) {
                var me = this;
                var placementAlias = placementConfig.alias;
                var placement = placementMap[placementAlias];
                if (placement) {
                    var stratMessage = placement['strat_message'];
                    var goodIds = [];
                    var goodLinks = [];
                    for (var i = 0; i < placement['recs'].length; i++) {
                        goodIds.push(placement['recs'][i]['pid']);
                        goodLinks.push(placement['recs'][i]['ct_url']);
                    }
                    $.ajax({
                        url: '/similarGoodsListBlock',
                        type: 'POST',
                        dataType: 'html',
                        data: {headerText: ((stratMessage == null) ? "" : stratMessage), goodIds: goodIds.join("\t"), type: placementConfig.type}
                    }).success(function (data) {
                        me.rrPlacementsInfo[placementAlias] = {
                            data: data,
                            goodIds: goodIds,
                            goodLinks: goodLinks
                        };
                        me.renderSimilarGoodsListBlock(placementConfig);
                    }).fail(function (data) {
                        console.log('Error /similarGoodsListBlock');
                        me.rrPlacementsInfo[placementAlias] = {
                            error: true
                        };
                        me.renderSimilarGoodsListBlock(placementConfig);
                    });
                } else {
                    me.rrPlacementsInfo[placementAlias] = {
                        error: true
                    };
                    me.renderSimilarGoodsListBlock(placementConfig);
                }
            };

            this.getSatelliteCategoryGoodsListBlock = function (placementMap, placementConfig) {
                var me = this;
                var placementAlias = placementConfig.alias;
                var placement = placementMap[placementAlias];
                if (placement) {
                    var stratMessage = placement['strat_message'];
                    var goodIds = [];
                    var goodLinks = [];
                    var goodCategoryNames = [];
                    for (var i = 0; i < placement['recs'].length; i++) {
                        goodIds.push(placement['recs'][i]['pid']);
                        goodLinks.push(placement['recs'][i]['ct_url']);
                        var categoryName = placement['recs'][i]['productcategories'];
                        goodCategoryNames.push(categoryName ? categoryName : "");
                    }
                    $.ajax({
                        url: '/satelliteCategoryGoodsListBlock',
                        type: 'POST',
                        dataType: 'html',
                        data: {headerText: ((stratMessage == null) ? "" : stratMessage), goodIds: goodIds.join("\t"), goodCategoryNames: goodCategoryNames.join("\t")}
                    }).success(function (data) {
                        me.rrPlacementsInfo[placementAlias] = {
                            data: data,
                            goodIds: goodIds,
                            goodLinks: goodLinks
                        };
                        me.renderSatelliteCategoryGoodsListBlock(placementConfig);
                    }).fail(function (data) {
                        console.log('Error /satelliteCategoryGoodsListBlock');
                        me.rrPlacementsInfo[placementAlias] = {
                            error: true
                        };
                        me.renderSatelliteCategoryGoodsListBlock(placementConfig);
                    });
                } else {
                    me.rrPlacementsInfo[placementAlias] = {
                        error: true
                    };
                    me.renderSatelliteCategoryGoodsListBlock(placementConfig);
                }
            };

            this.initErrorGoodsListBlocks = function () {
                var me = this;
                //Home Page
                    me.rrPlacementsInfo[me.placementsDictionary.homePageTop.config.alias] = {error: true};
                    me.rrPlacementsInfo[me.placementsDictionary.homePageBottom.config.alias] = {error: true};
                            };

            this.getGoodsListBlocks = function (placementMap) {
                var me = this;
                //Home Page
                    me.getTopGoodsListBlock(placementMap, me.placementsDictionary.homePageTop.config);
                    me.getSuperGoodsListBlock(placementMap, me.placementsDictionary.homePageBottom.config);
                            };

            this.renderGoodsListBlocks = function () {
                var me = this;
                //Home Page
                    me.renderTopGoodsListBlock(me.placementsDictionary.homePageTop.config);
                    me.renderSuperGoodsListBlock(me.placementsDictionary.homePageBottom.config);
                            };
        }

        RR_COUNTER = new RichRelevanceCounter();
        if (typeof RR === "object") {
            RR.jsonCallback = function () {
                //handle RR recommendations
                (function () {
                    if (RR.data.JSON.placements.length > 0) {
                        var placementMap = RR_COUNTER.transformPlacementListByNameToMapByAliases(RR.data.JSON.placements);
                        RR_COUNTER.getGoodsListBlocks(placementMap);
                    } else {
                        RR_COUNTER.initErrorGoodsListBlocks();
                        RR_COUNTER.renderGoodsListBlocks();
                    }
                })();
            }
        } else {
            RR_COUNTER.initErrorGoodsListBlocks();
            RR_COUNTER.renderGoodsListBlocks();
        }

            if (typeof RR === "object") {
                R3_COMMON = new r3_common();
                R3_COMMON.setApiKey('acde6ffcd2830223');
                R3_COMMON.setBaseUrl(window.location.protocol+'//recs.richrelevance.com/rrserver/');
                R3_COMMON.setClickthruServer(window.location.protocol+"//"+window.location.host);
                R3_COMMON.setSessionId('0B4544666BF72D34B9A57F79F7B04367.api14');
               // R3_COMMON.setUserId('0B4544666BF72D34B9A57F79F7B04367.api14');
                R3_COMMON.setRegionId('18413');
                    R3_COMMON.addSegment('Region', '18413');

                //Home Page
                    R3_HOME = new r3_home();
                    R3_COMMON.addPlacementType(RR_COUNTER.placementsDictionary.homePageTop.config.placementName);
                    R3_COMMON.addPlacementType(RR_COUNTER.placementsDictionary.homePageBottom.config.placementName);
                
                try {
                    rr_flush_onload();
                    r3();
                } catch (err) {
                    console.log("RR call failed");
                    RR_COUNTER.initErrorGoodsListBlocks();
                    RR_COUNTER.renderGoodsListBlocks();
                }
            }
    </script>



