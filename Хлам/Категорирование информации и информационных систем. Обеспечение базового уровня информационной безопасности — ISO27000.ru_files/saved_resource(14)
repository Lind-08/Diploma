                },
                homePageBottom: {
                    config: {
                        alias: 'homePageBottom',
                        placementName: 'home_page.bottom',
                        blockSelector: 'section#mainPageSuperGoods',
                        pageName: 'mainPage'
                    }
                },
                searchPageTop: {
                    config: {
                        alias: 'searchPageTop',
                        placementName: 'search_page.top',
                        blockSelector: 'section#searchPageNotFoundTopGoods'
                    }
                },
                categoryPageLevel1Top: {
