<script type="text/javascript" src="//2b22s2p.fast.ulmart.ru/resources/desktop.bundles/common/_common.min.js?69" ></script><script type="text/javascript">
    $(function () {
        var $token = $("meta[name='_csrf']");
        var $header = $("meta[name='_csrf_header']");
        if($token.length == 0 || $header.length == 0) {
            console.log("CSRF metadata was not found");
        } else {
            var token = $token.attr("content");
            var header = $header.attr("content");
            $(document).ajaxSend(function (e, xhr, options) {
                xhr.setRequestHeader(header, token);
            });
        }
    });
</script>
    <!--[if !IE]><!-->
    <script>
        var isIE1011 = window.navigator.msPointerEnabled;
        if (isIE1011) {
            document.documentElement.className += ' ie10 ie11';
        }
    </script>
