    <script charset="utf-8" type="text/javascript">

        function RichRelevanceCounter() {
            this.getScope = function () {
                return this;
            };

            this.rrPlacementsInfo = [];

            this.placementsDictionary = {
                homePageTop: {
                    config: {
                        alias: 'homePageTop',
                        placementName: 'home_page.top',
                        blockSelector: 'section#mainPageTopGoods'
