|setTimeout(function(){var a=A(!0);B.caret(a),h()},0)}),e&&f&&B.off("input.mask").on("input.mask",u),A()})}})});function authorization(form){

    $(document).ready(function(){
        var $form = $(form);

        $form.validationEngine({
            scroll : false,

            customFunctions : {
                issetEmail : function(field, rules, i, options) {
                    return issetEmail(field, rules, i, options);
                },
                checkUser : function(field, rules, i, options) {
                    return checkUser(field, rules, i, options);
                }
            }
        });

        $form.find('.switcher').click(function(){

            if(!$form.find('.switcher input').prop('checked')){
                $form.find('input[name="password"]').attr('type', 'password');
            }
            else{
                $form.find('input[name="password"]').attr('type', 'text');
            }
        });
    });
}function checkUser(field, rules, i, options) {
    var userNotExists = false,
        message = '',
        $form = $(field).parents('form');

    $.ajax({
        type		: 'POST',
        url			: '/application/ajax/public/A_Guest.php',
        async		: false,

        data		: {
            action : 'checkUser',
            email : $form.find('input[name="email"]').val(),
            password : field.val()
        },
        dataType	: 'json',

        success		: function(resp){

            if(!resp['success']){
                userNotExists = true;
                message = resp['message'];
            }
        }
    });
    if (userNotExists) {
        return message;
    }
}function issetEmail(field, rules, i, options){
    var issetEmail = true;

    $.ajax({
        type		: 'POST',
        url			: '/application/ajax/public/A_Guest.php',
        async		: false,

        data		: {
            action : 'issetEmail',
            email : field.val()
        },
        dataType	: 'json',

        success		: function(resp){

            if(resp['success']){
                issetEmail = resp['issetEmail'];
            }
        }
    });
    if (!issetEmail) {
        return 'Пользователя с таким email не существует';
    }
}</script>
