<script type="text/javascript">
    function changeCountry(){

    $.ajax({
            type: 'POST',
            url: '/application/ajax/public/A_Country.php',
            async: false,
            data: {
                action: 'getCountryPopup'
            },
            dataType: 'json',

            success: function (resp) {

                if (resp['success']) {
                    $('#change_region').remove();
                    $('body').append(resp['popup_regions']);
                    open_popups('change_region');
                }
            }
        }
    );
}$(document).ready(function () {
    var searchInput = $('.search-input');
    var popupSearch = $("#popup-search");

    searchInput.bind('keyup click', function () {
        old_search_phrase = typeof old_search_phrase == 'undefined' ? null : old_search_phrase;
        search_phrase = $(this).val();

        if (search_phrase.length >= 3 && search_phrase.length <= 50 && (search_phrase != old_search_phrase || !popupSearch.is(':visible'))) {
            old_search_phrase = search_phrase;

            $.ajax({
                type: 'POST',
                url: '/application/ajax/public/A_LiveSearch.php',
                data: 'search_phrase=' + search_phrase,
                dataType: 'json',
                success: function (resp) {

                    if (resp['success']) {

                        $("#search-result").html(resp['popup_search']);
                        $('#go-to-search').attr('onclick', 'goToUrl("/?s=' + search_phrase + '")');
                        showSearch();
                    }
                }
            });
        }
        else if (search_phrase.length < 3 || search_phrase.length > 50) {
            hideSearch();
            old_search_phrase = '';
        }
    });
    $('#popup-search button').click(function(){
        $('.search input[type="submit"]').trigger('click');
    });
    popupSearch.hover(
        function(){
            searchInput.off('focusout', hideSearch);
        },
        function(){

            if(searchInput.is(':focus')){
                searchInput.on('focusout', hideSearch);
            }
            else{
                hideSearch();
            }
        }
    );
    searchInput.on('focusout', hideSearch);

    function showSearch(){

        if (popupSearch.css("display") != "block") {
            $("#popup-search").animate({height: "show"}, 1000);
        }
    }
    function hideSearch(){
        $('#popup-search').animate({height: "hide"}, 250);
    }
});

function deleteProduct(idProduct, idComplect){

    $.ajax({
        type		: 'POST',
        url			: '/application/ajax/public/A_Basket.php',
        async		: false,

        data		: {
            action  : 'deleteProduct',
            id_product : idProduct,
            id_complect : idComplect
        },
        dataType	: 'json',

        success		: function(resp){

            if(resp['success']){
                $('.icon-basket').html(resp['mini_basket']);
                var $basketList = $('.basket_list');

                if(0 < $basketList.length){
                    $basketList.html(resp['items_basket']);
                    $('.all_summ span').text(resp['summ']);
                    basketCabinet.eventHelp($basketList);
                    basketCabinet.eventPlusMinus($basketList);
                    basketCabinet.eventSetCount($basketList);
                    basketCabinet.eventDeleteItem($basketList);
                }
            }
        }
    });
}var AjaxData = function(){
    this.methods = {};

    this.getData = function(){
        var obj = this;
        var data = {methods : {}};

        for(var i in obj.methods){
            data.methods[i] = obj.methods[i]['data'];
        }
        if(Object.keys(data.methods).length < 1){
            return false;
        }
        $.ajax({
                type: 'POST',
                url: '/application/ajax/public/A_Data.php',
                async: true,
                data: data,
                dataType: 'json',

                success: function (resp) {

                    for(var i in obj.methods){

                        try{
                            if(typeof  obj.methods[i]['success'] == 'function'){
                                obj.methods[i]['success'](resp[i]);
                            }
                        }
                        catch(e){
                            console.log('Error in AjaxData method ' + i);
                        }
                    }
                }
            }
        );
    }
};
ajaxData = new AjaxData();ajaxData.methods['getMiniBasket'] = {
    data : 1,
    success : function(resp){
        $('.icon-basket').html(resp['mini_basket']);
    }
};function createCookie(name,value,days){
	if (days){
		var date = new Date();
		
		date.setTime(date.getTime()+(days*24*60*60*1000));
		var expires = "; expires="+date.toGMTString();
	}
	else{
		var expires = "";
	}
	
	document.cookie = name+"="+value+expires+"; path=/";
}

function readCookie(name) 
{
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
	  
		var c = ca[i];
		while (c.charAt(0)==' ') {
			c = c.substring(1,c.length);
		}
		if (c.indexOf(nameEQ) == 0) {
			return c.substring(nameEQ.length,c.length);
		}
	}
	return '';
}

function eraseCookie(name) {
	createCookie(name,"",-1);
}  
 

