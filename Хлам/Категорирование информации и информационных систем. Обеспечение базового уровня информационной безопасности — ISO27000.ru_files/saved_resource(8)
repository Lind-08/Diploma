		}
	}
    
/* ------------------ Ymrktru ------------------------*/
	var cuc=readCookie('upfone');

	if (-1 < window.location.search.toString().indexOf('ymrk') || cuc=='ymrk' || ot_ref=='ymrk' ){
        readCookie('city')=='Moscov' ? replacePhone('+7 495 777 14 92') : 0;
        readCookie('city')=='St_Petersburg' ? replacePhone('+7 812 244 69 83') : 0;
        readCookie('city')=='Kiev' ? replacePhone('+38 044 362 19 07') : 0;
        cuc == 'ymrk' ? 0 : createCookie('upfone', 'ymrk', '90');
	}
    function replacePhone(phone){
        $('.phone-replace').text(phone);
        $('.phone-remove').remove();
    }
});
function authSocialNetwork(provider) {
    var width = screen.width / 2,
        height = screen.height / 2,
        top = (screen.height - height) / 2,
        left = (screen.width - width) / 2;

    //var width = 580, heigth = 304,
    
    var openWindow = window.open('/user/social_network/' + provider, 'Авторизация ' + provider,
            'width=' + width + ',height=' + height + ', top=' + top + ',left='
            + left +
            ', resizable=yes, scrollbars=yes, status=no, location=no'
        );
    openWindow.focus();

    var idInterval = setInterval(function(){

        try{
            var complete = openWindow.document.getElementById('result').getAttribute('data-complete');

            if(complete === 'ok'){
                openWindow.close();
                clearInterval(idInterval);
                goToUrl('/user/cabinet/data/');
            }
            else if(complete === 'error'){
                openWindow.close();
                clearInterval(idInterval);

                popupAlert(
                    '<div><p style="color: red;text-align: justify;margin-bottom: 20px">К сожалению, авторизация через выбранную социальную сеть временно недоступна.</p>' +
                    '<p style="color: red;text-align: justify">Попробуйте авторизоваться позже или выбрать другую социальную сеть</p></div>'
                );
            }
        }
        catch(e){
            //console.log(e);
        }
    }, 1000);
}$(document).ready(function () {
    // Slider Banner
    $('#slider-banner').bxSlider({
        mode: 'fade',
        auto: true,
        pause: 6000,
        // controls: false,
        slideWidth: 1170,
        nextSelector: '#slider-next-ban',
        prevSelector: '#slider-prev-ban'
    });
    // Slider with goods
    var sliderUsa = $('#slider-usa').bxSlider({
        minSlides: 1,
        maxSlides: 4,
        slideWidth: 285,
        // auto: true,
        pager: false,
        nextSelector: '#slider-next-usa',
        prevSelector: '#slider-prev-usa',
        moveSlides: 1
    });
    var sliderJp = $('#slider-jp').bxSlider({
        minSlides: 1,
        maxSlides: 4,
        slideWidth: 285,
        moveSlides: 1,
        // auto: true,
        pager: false,
        nextSelector: '#slider-next-jp',
        prevSelector: '#slider-prev-jp'
    });
    var sliderFormat = $('#slider-format').bxSlider({
        minSlides: 1,
        maxSlides: 4,
        slideWidth: 285,
        moveSlides: 1,
        // auto: true,
        pager: false,
        nextSelector: '#slider-next-format',
        prevSelector: '#slider-prev-format'
    });

    function slideShow() {
        if ($(window).width() <= '960') {
            sliderUsa.reloadSlider();
            sliderJp.reloadSlider();
            //sliderFormat.reloadSlider();
        } else {
            // sliderUsa.reloadSlider();
            // sliderJp.reloadSlider();
            // sliderFormat.reloadSlider();
            sliderUsa.destroySlider();
            sliderJp.destroySlider();
            //sliderFormat.destroySlider();
        }
    }

    $(window).on('load resize', slideShow);
});

function goToCompare(){

    $.ajax({
        type		: 'POST',
        url			: '/application/ajax/public/A_Compare.php',
        async		: false,

        data		: {
            action : 'getCountCompare'
        },
        dataType	: 'json',

        success		: function(resp){
            $('#compare_start').remove();

            if(!resp['success']) return false;

            if(typeof resp['start_popup'] == 'undefined'){
                window.location = '/compare/';
            }
            else{
                $('body').append(resp['start_popup']);
                open_popups('compare_start');
            }
        }
    });
